{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceRoot}",
            "executable": "./build/debug/lv_stm32f746.elf",
            "device": "STM32F746NGH6",
            "configFiles": [
                "interface/stlink.cfg",
                "target/stm32f7x.cfg"
            ],
            "name": "Debug (OpenOCD)",
            "request": "launch",
            "type": "cortex-debug",
            "showDevDebugOutput": true,
            "svdFile": "${workspaceRoot}/.vscode/STM32F746.svd",
            "servertype": "openocd",
            "runToMain": true,
            "preRestartCommands": [
                "file ./build/debug/lv_stm32f746.elf",
                "enable breakpoint",
                "monitor reset halt",
                "load"
            ]
        }
    ]
}